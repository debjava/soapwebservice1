	</servlet>

	<servlet-mapping>
		<servlet-name>HttpPostTextServlet</servlet-name>
		<url-pattern>/HttpPostTextServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>HttpXMLServlet</servlet-name>
		<servlet-class>com.ddlab.rnd.httppost.server.HttpXMLServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>HttpXMLServlet</servlet-name>
		<url-pattern>/HttpXMLServlet</url-pattern>
	</servlet-mapping>

