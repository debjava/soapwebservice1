	</servlet>

	<servlet-mapping>
		<servlet-name>HttpPostHtmlServlet</servlet-name>
		<url-pattern>/HttpPostHtmlServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>HttpPostXmlServlet</servlet-name>
		<servlet-class>com.ddlab.rnd.httppost.server.HttpPostXmlServlet</servlet-class>
